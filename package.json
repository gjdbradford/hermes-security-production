{
  "name": "hermes-security",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://gjdbradford.github.io/hermes-security-production/",
  "scripts": {
    "dev": "vite",
    "dev:staging": "vite --mode staging",
    "dev:production": "vite --mode production",
    "build": "vite build",
    "build:staging": "VITE_DEPLOY_ENV=staging vite build",
    "build:production": "VITE_DEPLOY_ENV=production vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview",
    "assets:upload": "npx tsx scripts/upload-assets.ts",
    "assets:upload-manifest": "npx tsx scripts/upload-manifest-assets.ts",
    "update-favicon": "npx tsx scripts/update-favicon.ts",
    "assets:status": "npx tsx scripts/asset-manager.ts status",
    "assets:sync": "npx tsx scripts/asset-manager.ts sync",
    "test:routing": "npx tsx scripts/test-routing.ts",
    "assets:watch": "npx tsx scripts/asset-manager.ts watch",
    "test:cdn": "npx tsx scripts/test-cdn-integration.ts",
    "test:fixes": "npx tsx scripts/test-fixes.ts",
    "test:all-envs": "npx tsx scripts/test-all-environments.ts",
    "test:captcha": "npx tsx scripts/test-captcha-integration.ts",
    "validate:cta": "npx tsx scripts/validate-cta-rules.ts",
    "validate:router": "npx tsx scripts/validate-router-navigation.ts",
    "quality:check": "npm run lint && npm run type-check && npm run format:check && npm run validate:cta && npm run validate:router && npm run test:run",
    "quality:fix": "npm run lint:fix && npm run format",
    "pre-commit": "npm run quality:check && npm run build:staging && npm run build:production",
    "pre-push": "npm run quality:check && npm run test:routing && npm run test:all-envs",
    "security:audit": "npm audit --audit-level=high",
    "security:secrets": "echo 'Secret detection requires Python detect-secrets tool'",
    "validate:build": "npm run build:staging && npm run build:production",
    "validate:all": "npm run quality:check && npm run validate:build && npm run test:routing && npm run test:all-envs",
    "test:quality-gates": "./scripts/test-quality-gates-local.sh",
    "cors-proxy": "npx tsx scripts/cors-proxy-server.ts",
    "local-api": "npx tsx scripts/local-api-server.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky",
    "db:migrate": "npx tsx scripts/migrate-database.ts up",
    "db:migrate:down": "npx tsx scripts/migrate-database.ts down",
    "db:migrate:status": "npx tsx scripts/migrate-database.ts status",
    "db:migrate:dev": "NODE_ENV=development npx tsx scripts/migrate-database.ts up",
    "db:migrate:staging": "NODE_ENV=staging npx tsx scripts/migrate-database.ts up",
    "db:migrate:production": "NODE_ENV=production npx tsx scripts/migrate-database.ts up",
    "db:health": "npx tsx scripts/database-health-check.ts",
    "db:health:dev": "NODE_ENV=development npx tsx scripts/database-health-check.ts",
    "db:health:staging": "NODE_ENV=staging npx tsx scripts/database-health-check.ts",
    "db:health:production": "NODE_ENV=production npx tsx scripts/database-health-check.ts",
    "db:seed": "npx tsx scripts/seed-database.ts"
  },
